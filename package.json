{
	"name": "interactive_output_c_cpp",
	"displayName": "interactive_output_c_cpp",
	"description": "C/C++ Interactive Output for VS Code",
	"version": "0.0.1",
	"publisher": "derekpage",
	"repository": "https://github.com/cacttus/vscode_interactive_output_c_cpp",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	], 
	"activationEvents": [
		"onCommand:extension.interactive_output_c_cpp"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.interactive_output_c_cpp",
				"title": "Interactive Output C/CPP"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.20.37",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.2.2"
	},
	"dependencies": {
		"vscode": "^1.1.37"
	}
}
